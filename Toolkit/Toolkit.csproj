<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.1;netstandard2.0</TargetFrameworks>
        <OutputType>Library</OutputType>
        <LangVersion>8</LangVersion>
        <VersionSuffix>-$(Configuration)</VersionSuffix>
        <RootNamespace>Hel.Toolkit</RootNamespace>
    </PropertyGroup>
    <PropertyGroup>
        <PackageId>Hel.Toolkit</PackageId>
        <PackageTags>hel;hel.toolkit; core;.net</PackageTags>
        <Version>0.5.22</Version>
        <Authors>Hel_Engine</Authors>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>

    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    </ItemGroup>
</Project>
